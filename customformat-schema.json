        "negate": false,
        "required": false,
        "fields": [
          {
            "order": 0,
            "name": "value",
            "label": "Regular Expression",
            "helpText": "Custom Format RegEx is Case Insensitive",
            "value": "subs?",
            "type": "textbox",
            "advanced": false
          }
        ]
      },
      {
        "name": "Hardcoded Subs",
        "implementation": "ReleaseTitleSpecification",
        "implementationName": "Release Title",
        "infoLink": "https://wiki.servarr.com/radarr/settings#custom-formats-2",
        "negate": false,
        "required": false,
        "fields": [
          {
            "order": 0,
            "name": "value",
            "label": "Regular Expression",
            "helpText": "Custom Format RegEx is Case Insensitive",
            "value": "\\b(?<hcsub>(\\w+SUBS?)\\b)|(?<hc>(HC|SUBBED))\\b",
            "type": "textbox",
            "advanced": false
          }
        ]
      },
      {
        "name": "Surround Sound",
        "implementation": "ReleaseTitleSpecification",
        "implementationName": "Release Title",
        "infoLink": "https://wiki.servarr.com/radarr/settings#custom-formats-2",
        "negate": false,
        "required": false,
        "fields": [
          {
            "order": 0,
            "name": "value",
            "label": "Regular Expression",
            "helpText": "Custom Format RegEx is Case Insensitive",
            "value": "DTS.?(HD|ES|X(?!\\D))|TRUEHD|ATMOS|DD(\\+|P).?([5-9])|EAC3.?([5-9])",
            "type": "textbox",
            "advanced": false
          }
        ]
      },
      {
        "name": "Preferred Words",
        "implementation": "ReleaseTitleSpecification",
        "implementationName": "Release Title",
        "infoLink": "https://wiki.servarr.com/radarr/settings#custom-formats-2",
        "negate": false,
